{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "MyBGList": "Information"
    },
    "Console": {
      "FormatterOptions": {
        "UseUtcTimeStamp": true,
        "TimestampFormat": "HH:mm",
        "SingleLine": true
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "MyBGList": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "UseDeveloperExceptionPage": false,
  "AllowedOrigins": "*"
}
